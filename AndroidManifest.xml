<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nuo.activity"
    android:versionCode="1"
    android:versionName="0.2.1">

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 往SDCard写入数据权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 在SDCard中创建与删除文件权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!-- 震动权限 -->
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.READ_CALL_LOG" />
    <uses-permission android:name="android.permission.WRITE\_CALL_LOG" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.WRITE_SMS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.UNINSTALL_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.READ_SETTINGS" />

    <!-- 屏幕设置多分辨率支持-->
    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"></supports-screens>
    <application
        android:name="com.nuo.application.AppApplication"
        android:allowBackup="true"
        android:configChanges="orientation|keyboardHidden"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:logo="@drawable/icon"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.nuo.activity.StartActivity"
            android:theme="@android:style/Theme.Light.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.DIAL" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:mimeType="vnd.android.cursor.item/phone" />
                <data android:mimeType="vnd.android.cursor.item/person" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.DIAL" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="voicemail" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.PBTOFRONT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.DIAL" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.DIAL" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="tel" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="qqbookdial" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:mimeType="vnd.android.cursor.dir/calls" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.CALL_BUTTON" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.android.contacts.action.LIST_CONTACTS" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.TAB" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="vnd.android-dir/mms-sms" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="vnd.android-dir/msms" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="contacts"
                    android:mimeType="vnd.android.cursor.dir/person" />
                <data
                    android:host="com.android.contacts"
                    android:mimeType="vnd.android.cursor.dir/contact" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.android.phone.action.RECENT_CALLS" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nuo.activity.MainActivity"
            android:theme="@android:style/Theme.Light.NoTitleBar"></activity>
        <activity android:name="com.nuo.activity.FrameActivity">
            <intent-filter>
                <action android:name="com.android.contacts.action.LIST_DEFAULT" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.TAB" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.android.contacts.action.LIST_ALL_CONTACTS" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.TAB" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.android.contacts.action.LIST_CONTACTS_WITH_PHONES" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.TAB" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.android.contacts.action.LIST_STARRED" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.TAB" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.android.contacts.action.LIST_FREQUENT" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.TAB" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.android.contacts.action.LIST_STREQUENT" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.TAB" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HuangyeActivity"
            android:label="@string/title_activity_huangye"></activity>
        <activity android:name="com.nuo.activity.KeyActivity"></activity>
        <activity
            android:name="com.nuo.activity.CheckinActivity"
            android:label="@string/title_activity_checkin"></activity>
        <activity
            android:name="com.nuo.activity.MyActivity"
            android:label="@string/title_activity_my"
            android:theme="@android:style/Theme.Light.NoTitleBar"></activity>
        <activity
            android:name="com.nuo.activity.MoreActivity"
            android:label="@string/title_activity_more"></activity>
        <activity android:name=".HuangyeListActivity"></activity>
        <activity
            android:name="com.nuo.activity.SearchMoreActivity"
            android:label="@string/title_activity_search_more"></activity>
        <activity
            android:name=".InfoDetailActivity"
            android:label="@string/title_activity_info_details"></activity>
        <activity
            android:name=".ShopDetailActivity"
            android:label="@string/title_activity_shop_details"></activity>
        <activity
            android:name=".HuangyeLevelTwoActivity"
            android:label="@string/title_activity_sou_quan_cheng"></activity>
        <activity
            android:name="com.nuo.activity.RegistrationActivity"
            android:label="@string/title_activity_registration"></activity>
        <activity
            android:name="com.nuo.activity.LoginActivity"
            android:label="@string/title_activity_login"></activity>
        <activity
            android:name="com.nuo.activity.ShopDetailsCommentActivity"
            android:label="@string/title_activity_shop_detailsdianping"></activity>
        <activity
            android:name="com.nuo.activity.RankingList_more"
            android:label="@string/title_activity_paihangbang_more"></activity>
        <activity android:name="com.nuo.activity.SmsActivity"></activity>
        <activity android:name="com.nuo.activity.NewSmsActivity"></activity>
        <activity android:name="com.nuo.activity.BatchSendSmsActivity"></activity>
        <activity android:name="com.nuo.activity.ContactsDetailActivity"></activity>
        <activity android:name="com.nuo.activity.AddContactsActivity"></activity>
        <activity android:name="com.nuo.activity.ChatActivity"></activity>
        <activity android:name="com.nuo.activity.ToolActivity"></activity>
        <activity
            android:name="com.nuo.activity.ImagePagerActivity"
            android:theme="@android:style/Theme.Light.NoTitleBar"></activity>
        <activity
            android:name="com.nuo.activity.RelationActivity"
            android:windowSoftInputMode="stateHidden|stateUnchanged"></activity>
        <activity
            android:name=".ChangeFabuTypeActivity"
            android:label="@string/change_fabu_type" />
        <activity
            android:name="com.nuo.activity.FeedBackActivity"
            android:theme="@android:style/Theme.NoTitleBar" />
        <activity
            android:name=".AboutActivity"
            android:theme="@android:style/Theme.NoTitleBar" />
        <activity
            android:name=".UserCenterActivity"
            android:theme="@android:style/Theme.NoTitleBar" />
        <activity
            android:name=".SearchActivity"
            android:label="信息查询" />
        <activity
            android:name=".FabuActivity"
            android:windowSoftInputMode="stateHidden|stateUnchanged"
            android:label="发布信息" />
        <activity
            android:name=".ChangePicActivity"
            android:label="选择图片"
            android:theme="@android:style/Theme.Translucent.NoTitleBar"
            />
        <activity android:name=".CenterActivity" />
        <activity
            android:name=".NoteBookActivity"
            android:label="记事本" />
        <activity
            android:name=".AddNoteBookActivity"
            android:label="添加笔记" />
        <activity
            android:name=".PreviewNoteBook"
            android:label="预览笔记" />

        <receiver android:name="com.nuo.broadcastreceiver.BootBroadcastReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
    </application>
</manifest>